{
  "compilerOptions": {

    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "jsx": "react-jsx"
  },
  "exclude": ["node_modules", "dist", "frontend"]
}
